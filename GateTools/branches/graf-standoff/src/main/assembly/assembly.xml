<assembly>
  <id>GATE</id>
  <formats>
    <format>jar</format>
  </formats>
  <files>
  	<file>
	  	<outputDirectory></outputDirectory>
  		<source>${basedir}/src/main/resources/creole.xml</source>
  	</file>
  </files>
  <includeBaseDirectory>false</includeBaseDirectory>
  <dependencySets>
    <dependencySet>
      <outputDirectory></outputDirectory>
      <outputFileNameMapping></outputFileNameMapping>
      <unpack>true</unpack>
      <scope>runtime</scope>
      <includes>
      	<include>org.anc:common</include>
      	<include>org.xces:xces-parser</include>
      	<include>org.tc37sc4.graf:graf-api</include>
      </includes>
      <excludes>
      	<exclude>gate:gate</exclude>
      </excludes>
    </dependencySet>
  </dependencySets>
  <fileSets>
    <fileSet>
      <directory>target/classes</directory>
      <outputDirectory></outputDirectory>
    </fileSet>
  </fileSets>
</assembly>